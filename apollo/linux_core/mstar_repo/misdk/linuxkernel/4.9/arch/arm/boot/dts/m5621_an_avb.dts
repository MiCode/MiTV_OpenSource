/dts-v1/;

/ {
	model = "M5621";
	compatible = "arm,foundation-arm", "arm,m5621";
	interrupt-parent = <0x1>;
	#address-cells = <0x2>;
	#size-cells = <0x2>;

	chosen {
		bootargs = "root=/dev/mmcblk0p4 rootfstype=ext4 rw rootwait init=/sbin/init ip=dhcp console=ttyS0,115200,LX_MEM=0x02000000";
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,armv8";
			reg = <0>;
			enable-method = "mstar-spin-table";
			cpu-release-addr = <0x100510>;
		};

		cpu@1 {
			device_type = "cpu";
			compatible = "arm,armv8";
			reg = <1>;
			enable-method = "mstar-spin-table";
			cpu-release-addr = <0x100510>;
		};
		cpu@2 {
			device_type = "cpu";
			compatible = "arm,armv8";
			reg = <2>;
			enable-method = "mstar-spin-table";
			cpu-release-addr = <0x100510>;
		};

		cpu@3 {
			device_type = "cpu";
			compatible = "arm,armv8";
			reg = <3>;
			enable-method = "mstar-spin-table";
			cpu-release-addr = <0x100510>;
		};

	};

	memory@20200000 {
		device_type = "memory";
		reg = <0x0 0x20200000 0x0 0x1f400000>;
	};

	memory@38C00000 {
		device_type = "memory";
		reg = <0x0 0x4dc00000 0x0 0x8a00000>;
	};

	memory@A0000000 {
		device_type = "memory";
		reg = <0x0 0xb2a00000 0x0 0x2d600000>;
	};

	interrupt-controller@16001000 {
		compatible = "arm,cortex-a15-gic", "arm,cortex-a9-gic";
		#interrupt-cells = <0x3>;
		#address-cells = <0x0>;
		interrupt-controller;
		reg = <0x0 0x16001000 0x0 0x1000 0x0 0x16002000 0x0 0x4000>;
		interrupts = <0x1 0x9 0xf04>;
		linux,phandle = <0x1>;
		phandle = <0x1>;
	};

	timer {
		compatible = "arm,mstar-timer-workaround","arm,armv8-timer";
		interrupts = <0x1 0xd 0xff01 0x1 0xe 0xff01 0x1 0xb 0xff01 0x1 0xa 0xff01>;
		clock-frequency = <0xb71b00>;
	};

	pmu {
		compatible = "arm,cortex-a53-pmu";
		interrupts = <0x0 0x8a 0x4>,
			<0x0 0x8c 0x4>,
			<0x0 0x8e 0x4>,
			<0x0 0x90 0x4>;
	};

	emac {
		compatible = "mstar-emac";
	};

    miu {
        compatible = "mstar-miu";
    };

        ir {
	        compatible = "mstar-ir";
	};

	mbx {
	        compatible = "mstar-mbx";
	};

	xc {
		compatible = "mstar-xc";
	};

	scaler {
		compatible = "mstar-scaler";
	};

	gflip {
		compatible = "mstar-gflip";
	};

	alsa {
		compatible = "Mstar-alsa";
	};

	mstar-rtc@0 {
		compatible = "mstar-rtc";
		index = <0>;
		xtal = <12000000>;
		freq = <1>;
	};

	Mstar-ehci-1 {
		compatible = "Mstar-ehci-1";
	};

	Mstar-ehci-2 {
		compatible = "Mstar-ehci-2";
	};

	Mstar-ehci-3 {
		compatible = "Mstar-ehci-3";
	};

  Mstar-udc {
  	compatible = "Mstar-udc";
  	interrupts = <0x0 0xc5 0x0>;
  	};

  Mstar-otg {
  	compatible = "Mstar-otg";
  	interrupts = <0x0 0xa6 0x0>, <0x0 0xa7 0x0>;
  	};

	Mstar-utopia2k-str {
		compatible = "Mstar-utopia2k-str";

		str-stage2 {
			functions = "ATSCStr",
				"DTMBStr",
				"DVBCStr",
				"DVBSStr",
				"DVBTStr",
				"DVBT2Str",
				"ISDBTStr";
		};

		mhl {
		  resume {
		    stage0 {
		      pre-condition = "hdmi_rx";
		    };
		  };
		};
		hdmi_rx {
		  resume {
		    stage0 {
		      post-condition = "hdmi_rx";
		    };
		  };
		};
		avd {
		  resume {
		    stage0 {
		      pre-condition = "vif";
		      post-condition = "avd";
		    };
		  };
		};
		vif {
		  resume {
		    stage0 {
		      post-condition = "vif";
		    };
		  };
		};
		vdec {
		  resume {
		    stage0 {
		      post-condition = "vdec";
		    };
		  };
		};

        miu {
          resume {
            stage0 {
              post-condition = "miu";
            };
          };
        };

		pq {
          		suspend {
		    		stage0 {
		      			post-condition = "pq";
		    		};
		  	};
		  	resume {
		    		stage0 {
		      			post-condition = "pq";
		      			pre-condition = "xc";
		    		};
		  	};
		};

		mvop {
			resume {
				stage0 {
					post-condition = "mvop";
				};
			};
		};

		pnl {
			suspend {
				stage0 {
				pre-condition = "xc";
				};
			};
			resume {
				stage0 {
				post-condition = "pnl";
				};
			};
		};

		ace {
			suspend {
				stage0 {
				post-condition = "ace";
				};
			};
			resume {
				stage0 {
				post-condition = "ace";
				pre-condition = "xc";
				};
			};
		};

		ve {
		  resume {
		    stage0 {
		      pre-condition = "miu","xc";
		    };
		  };
		};

		
		xc {
			suspend {
			stage0 {
				pre-condition = "pq","ace";
				post-condition = "xc";
				};
			};
			resume {
		 		stage0 {
				pre-condition = "vdec","avd","miu","mvop","pnl";
				post-condition = "xc";
			};
		  };
		};


	};

	Mstar-sys {
		firmware {
			audio {
				path = "/vendor/lib/utopia/";
			};
		};
	};

	firmware {
		android {
			compatible = "android,firmware";
			hardware ="m5621";
			revision ="1234";
			Serial   ="0000000000000000";
			Processor ="AArch32 Processor rev 3 (aarch32)";
			mode ="123456789";
			baseband ="123456789";
			bootloader ="123456789";

			vbmeta {
				compatible = "android,vbmeta";
				parts = "vbmeta,boot,system,vendor";
				by_name_prefix ="/dev/block/platform/mstar_mci.0/by-name/vbmeta";
			};
			fstab {
				compatible = "android,fstab";

				system {
					compatible = "android,system";
					dev = "/dev/block/platform/mstar_mci.0/by-name/system";
					type = "ext4";
					mnt_flags = "ro";
					fsmgr_flags = "wait,avb";
				};

				vendor {
					compatible = "android,vendor";
					dev = "/dev/block/platform/mstar_mci.0/by-name/vendor";
					type = "ext4";
					mnt_flags = "ro";
					fsmgr_flags = "wait,avb";
				};
			};
		};
	};

	Mstar-STR {
		mstar_mci {
			suspend-stage1 = "late";
			resume-stage1 = "early";
			policy = "async";
		};

		mmcblk {
			suspend-stage1 = "noirq";
			resume-stage1 = "noirq";
		};

		Mstar-utopia2k-str {
			suspend-stage1 = "late";
			resume-stage1 = "early";
			suspend-stage2 = "normal";
			resume-stage2 = "normal";
			policy = "async";
			resume-stage1-waitfor = "mstar_mci.0";
		};

		usbcore {
			suspend-stage1 = "late";
			resume-stage1 = "early";
		};

		Mstar-ehci {
			suspend-stage1 = "late";
			resume-stage1 = "early";
		};

		Mstar-udc {
			suspend-stage1 = "late";
			resume-stage1 = "early";
		};

		Mstar-otg {
			suspend-stage1 = "late";
			resume-stage1 = "early";
		};
	};

    gpio: gpio{
        compatible = "mstar,gpio";
        gpio-controller;
        #gpio-cells = <1>;
        interrupt-controller;
        skip-save-status = <13 45>;
        #interrupt-cells = <2>; /* cell name GPIO_PAD, trigger-type*/
    };

	/* example of consume a gpio pin with mapped irq
	foo{
		interrupt-parent = <&gpio>;
		interrupts = <0 LEVEL_HIGH>;
	}
	*/
	i2c1: i2c@1 {
		compatible = "mstar,swi2c";
		bus-index = <1>;
		sda-gpio = <142>;
		scl-gpio = <141>;
		speed-khz = <100>;
		def-delay = <100>;
		retries = <5>;
		staus = "okay";
		iic-mode = <0>;
		hw-port = <0>;
		pad-mux = <0>;
	};
};
