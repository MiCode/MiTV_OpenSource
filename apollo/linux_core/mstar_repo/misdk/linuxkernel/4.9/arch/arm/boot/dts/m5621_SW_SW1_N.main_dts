	i2c0: i2c@0 {
		compatible = "mstar,swi2c";
		bus-index = <0x1>;
		sda-gpio = <0x33>;
		scl-gpio = <0x32>;
		speed-khz = <0x64>;
		def-delay = <0x64>;
		retries = <0x5>;
		staus = "okay";
		iic-mode = <0x1>;
		hw-port = <0x3>;
		pad-mux = <0x1a>;
	};

	i2c1: i2c@1 {
		compatible = "mstar,swi2c";
		bus-index = <1>;
		sda-gpio = <142>;
		scl-gpio = <141>;
